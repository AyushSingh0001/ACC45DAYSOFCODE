# ACC45DAYSOFCODE
#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int a,b,c;
	    cin>>a>>b>>c;
	    int f=0;
	    for(int i=1;i<=a;i++){
	        if(c==0){
	            f=1;
	            break;
	        }
	        if(i*b==c){
	            f=1;
	            break;
	        }
	    }
	    if(f){
	        cout<<"Yes"<<endl;
	    }
	    else{
	        cout<<"No"<<endl;
	    }
	}
	return 0;
}
