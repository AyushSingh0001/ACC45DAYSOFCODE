# ACC45DAYSOFCODE
#include <iostream>
#include<cstdlib>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t>0){
	    int x,y,d;
	    cin>>x>>y>>d;
	    int ans=abs(x-y);
	    if(ans<=d){
	        cout<<"YES"<<endl;
	    }
	    else{
	        cout<<"NO"<<endl;
	    }
	    t--;
	}
	return 0;
}
