# ACC45DAYSOFCODE
#include <iostream>
using namespace std;

int main() {
    int t, i;
    cin>>t;
    for(i=0;i<t;i++)
    {
      int x,y,a,b;
      cin>>a>>b>>x>>y;
      if(a>b)

      {
          if((a-b)<=y)
          cout<<"yes"<<endl;
          else
          cout<<"no"<<endl;
      }

        else if(b>a)
        {
            if((b-a)<=x)
            cout<<"yes"<<endl;
            else
            cout<<"no"<<endl;
        }
       else if(a==b)
        cout<<"yes"<<endl;
    }

	return 0;
}

